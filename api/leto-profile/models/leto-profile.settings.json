{
  "kind": "collectionType",
  "collectionName": "leto_profiles",
  "info": {
    "name": "Leto_Profile",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "leto_name": {
      "type": "string",
      "required": true
    },
    "leto_bg": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "leto_links": {
      "type": "component",
      "repeatable": true,
      "component": "link-names-and-links.leto-links"
    },
    "leto_icons": {
      "type": "component",
      "repeatable": true,
      "component": "icons.leto-icons",
      "required": true
    },
    "leto_links_textColor": {
      "type": "string",
      "required": true
    },
    "leto_name_textColor": {
      "type": "string",
      "required": true
    },
    "leto_icons_color": {
      "type": "string",
      "required": true
    },
    "leto_links_bgColor": {
      "type": "string",
      "required": true
    },
    "letoSlug": {
      "type": "uid",
      "targetField": "leto_name"
    }
  }
}
